sudo -u renderer psql -d gis -c "select max(id) max_id, 'nodes' as name from planet_osm_nodes union select max(id), 'ways' from planet_osm_ways union select max(id), 'rels' from planet_osm_rels  union select max(osm_id), 'line' from planet_osm_line  union select max(osm_id), 'point' from planet_osm_point  union select max(osm_id), 'polygon' from planet_osm_polygon union select max(osm_id), 'roads' from planet_osm_roads order by 2;"
